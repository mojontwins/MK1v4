# flags:
# 1	- 
# 2 - COUNT_KILLABLE_ON

PLAYER_KILLS_ENEMY
	IF ENEMIES_KILLED_EQUALS #2
	IF NPANT_NOT 29
	THEN
		SOUND 11
		SOUND 2
		NEXT_LEVEL
	END
	
	IF ENEMIES_KILLED_EQUALS #2
	IF NPANT 29
	THEN
		SOUND 11
		SOUND 2
		WIN GAME
	END
END

